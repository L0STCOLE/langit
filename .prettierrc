{
	"trailingComma": "all",
	"useTabs": true,
	"tabWidth": 2,
	"printWidth": 110,
	"semi": true,
	"singleQuote": true,
	"bracketSpacing": true,
	"plugins": ["prettier-plugin-tailwindcss"],
	"tailwindConfig": "./app/tailwind-base.config.js",
	"tailwindFunctions": ["tw"],
	"overrides": [
		{
			"files": ["tsconfig.json", "jsconfig.json"],
			"options": {
				"parser": "jsonc"
			}
		}
	]
}
